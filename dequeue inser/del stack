#define MAX 10
#include <iostream>
using namespace std;

int Queue[MAX];
int front = -1, rear=-1;
void Enqueue(int val)
{
    if(rear == MAX - 1)
    cout<<"Queue is full";
    else{
        rear=rear+1;
        Queue[rear]=val;
        cout << "insert element"<<Queue [rear]<<endl;
    }
    if (rear==0)
    front=0;
}
void dequeue()
{
    if(front==-1)
    cout<<"Queue is empty";
    else
    {
        cout<<"deleted element"<<Queue[front];
        if(front==rear)
        front=rear=rear-1;
        else
        front=front+1;
    }
}
void peek()
{
    if(front==-1)
    cout<<"Queue is empty";
    else{
        cout<<"front element is"<<Queue[front]<<endl;
        cout<<"rear element is"<<Queue[rear]<<endl;
        
    }
}
int main()
{
    Enqueue(10);
    Enqueue(20);
    Enqueue(30);
    peek();
    dequeue();
    peek();
    return 0;
}
