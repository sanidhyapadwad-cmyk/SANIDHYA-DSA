#include <iostream>
#include <stack>
#include <string>
using namespace std;

int main()
{ 
    {
        stack<int> st;
        st.push(1);
        st.push(2);
        st.push(3);
        st.push(4);
       
        while(!st.empty()){
            cout << st.top() << " ";
            st.pop();
        }
        cout << endl;
    }
   
    {
        stack<string> st;
        st.push("a");
        st.push("b");
        st.push("c");
        st.push("d");
        
        while(!st.empty()){
            cout << st.top() << " ";
            st.pop();
        }
        cout << endl;
    }

    return 0;
}

